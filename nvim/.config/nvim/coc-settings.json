{
  "java.trace.server": "verbose",
  "java.format.settings.url": "/Users/magnus/Kode/asp-interpreter/allman-formatter.xml",
  "java.errors.incompleteClasspath.severity": "ignore",
  "coc.preferences.formatOnSaveFiletypes": [
    "haskell",
    "json",
    "css",
    "scss",
    "javascriptreact",
    "typescriptreact",
    "javascriptreact",
    "markdownreact",
    "markdown",
    "typescript",
    "javascript",
    "python",
    "elm"
  ],
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": [
        "haskell",
        "lhaskell"
      ]
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        "elm.json"
      ]
    }
  },
  "codeLens.enable": true
}
